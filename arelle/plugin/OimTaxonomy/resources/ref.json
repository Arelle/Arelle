            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Note",
                "value": "Notes can contain reference material; use this element when the note is published as a standalone document. There is a separate element for footnotes within other references."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Section",
                "value": "Section is used to capture information typically captured in sections of legislation or reference documents."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Subsection",
                "value": "Subsection is a subsection of the section part."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Paragraph",
                "value": "Paragraph is used to refer to specific paragraphs in a document."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Subparagraph",
                "value": "Subparagraph of a paragraph."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Clause",
                "value": "Sub component of a sub paragraph."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Subclause",
                "value": "Subcomponent of a clause in a paragraph."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Appendix",
                "value": "Refers to the name of an Appendix, which could be a number or text."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Example",
                "value": "Example captures examples used in reference documentation; there is a separate element for Exhibits."
