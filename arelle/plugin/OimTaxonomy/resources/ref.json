            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Chapter",
                "value": "For a publication that uses chapters, this part should be used to capture this information. Because chapters are not necessarily numbers, this is a string."
            },
            {
                "labelType": "xbrl:label",
                "language": "en",
                "relatedName": "ref:Article",
