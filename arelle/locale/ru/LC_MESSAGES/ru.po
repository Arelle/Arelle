msgstr "Факт %(fact)s является нулевым nonFraction и НЕ ДОЛЖЕН иметь первичное событие ix: nonFraction"

#: arelle\XhtmlValidate.py:359
msgid "Fact %(fact)s is a nil nonFraction and MUST not have an child elements or text"
msgstr "Факт %(fact)s является нулевым nonFraction и НЕ ДОЛЖЕН иметь дочерние элементы или текст"

#: arelle\XhtmlValidate.py:366
msgid "Fact %(fact)s is a non-nil nonFraction and MUST have exactly one ix:nonFraction child element or text."
msgstr "Факт %(fact)s является ненулевым nonFraction и ДОЛЖЕН иметь ровно один дочерний элемент ix: nonFraction или текст."

#: arelle\XhtmlValidate.py:374
msgid "Fact %(fact)s is a nil fraction and MUST not have an ancestor ix:fraction"
msgstr "Факт %(fact)s является нулевой фракцией и НЕ ДОЛЖЕН иметь исходное событие ix: fraction"

#: arelle\XhtmlValidate.py:380
msgid "Fact %(fact)s is a non-nil fraction and not have any child elements except ix:fraction, ix:numerator and ix:denominator: %(children)s"
msgstr "Факт %(fact)s - это ненулевая фракция и не имеет дочерних элементов, кроме ix: fraction, ix:numerator and ix:denominator: %(children)s"

#: arelle\XhtmlValidate.py:389
msgid "Fact %(fact)s has unrecognized transformation namespace %(namespace)s"
msgstr "Факт %(fact)s имеет неопознанное пространство имен преобразований %(namespace)s"

#: arelle\XhtmlValidate.py:394
msgid "Fact %(fact)s has unrecognized transformation name %(name)s"
msgstr "Факт %(fact)s имеет неопознанное имя преобразования %(name)s"

#: arelle\XhtmlValidate.py:416
msgid "Inline XBRL element name %(element)s is not valid"
msgstr "имя элемента встроенного XBRL %(element)s недействительно"

#: arelle\XhtmlValidate.py:425
msgid "Attribute %(attribute)s required on element ix:%(element)s"
msgstr "Атрибут %(attribute)s требуется для элемента ix: %(element)s"

#: arelle\XhtmlValidate.py:460 arelle\XhtmlValidate.py:467
msgid "%(element)s error %(error)s"
msgstr " %(element)s ошибка %(error)s"

#: arelle\XmlValidate.py:138
msgid "Element %(element)s fact %(fact)s type %(typeName)s is nil but element has not been defined nillable"
msgstr "Элемент %(element)s факт %(fact)s тип %(typeName)s нулевой, но элемент не определен nillable"

#: arelle\XmlValidate.py:163
msgid "Inline element %(element)s fact %(fact)s type %(typeName)s transform %(transform)s value error: %(value)s"
msgstr "встроенный элемент %(element)s факт %(fact)s тип %(typeName)s трансформирует %(transform)s ошибку значния: %(value)s"

#: arelle\XmlValidate.py:169
msgid "Fact %(fact)s has unrecognized transformation %(transform)s, value: %(value)s"
msgstr "Факт %(fact)s имеет неопознанное преобразование %(transform)s, значение: %(value)s"

#: arelle\XmlValidate.py:175
msgid "Element %(element)s has abstract declaration, value: %(value)s"
msgstr "Элемент %(element)s имеет абстрактную декларацию, значение: %(value)s"

#: arelle\XmlValidate.py:179
msgid "Element %(element)s error %(error)s value: %(value)s"
msgstr "Элемент %(element)s ошибка %(error)s значение: %(value)s"

#: arelle\XmlValidate.py:245
msgid "Element %(element)s type %(typeName)s missing required attributes: %(attributes)s"
msgstr "Элемент %(element)s тип %(typeName)s отсутствует требуемые атрибуты: %(attributes)s"

#: arelle\XmlValidate.py:260
msgid "Element %(element)s type %(typeName)s extraneous attributes: %(attributes)s"
msgstr "Элемент %(element)s тип %(typeName)s посторонние атрибуты: %(attributes)s"

#: arelle\XmlValidate.py:279
msgid "Element %(element)s is nil but has contents"
msgstr "Элемент %(element)s равен нулю, но имеет содержимое"

#: arelle\XmlValidate.py:504
msgid "Element %(element)s attribute %(attribute)s type %(typeName)s value error: %(value)s, %(error)s"
msgstr "Элемент %(element)s атрибут %(attribute)s тип %(typeName)s  ошибка значения: %(value)s, %(error)s"

#: arelle\XmlValidate.py:513
msgid "Element %(element)s type %(typeName)s value error: %(value)s, %(error)s"
msgstr "Элемент %(element)s тип %(typeName)s ошибка значения : %(value)s, %(error)s"

#: arelle\XmlValidateParticles.py:79
msgid "%(compositor)s(%(particles)s) %(element)s occurred %(occurrences)s times, minOccurs=%(minOccurs)s, maxOccurs=%(maxOccurs)s, within %(parentElement)s"
msgstr " %(compositor)s ( %(particles)s) %(element)s появился %(occurrences)s раз, minOccurs = %(minOccurs)s, maxOccurs = %(maxOccurs)s, внутри %(parentElement)s"

#: arelle\XmlValidateParticles.py:81
msgid "%(compositor)s(%(particles)s) content occurred %(occurrences)s times, minOccurs=%(minOccurs)s, maxOccurs=%(maxOccurs)s, within %(parentElement)s"
msgstr " %(compositor)s ( %(particles)s) появилось содержание %(occurrences)s раз, minOccurs = %(minOccurs)s, maxOccurs = %(maxOccurs)s, внутри %(parentElement)s"

#: arelle\XmlValidateParticles.py:89
msgid "All(%(particles)s) missing at %(element)s, within %(parentElement)s"
msgstr "Все ( %(particles)s) отсутствуют в %(element)s, внутри %(parentElement)s"

#: arelle\XmlValidateParticles.py:101
msgid "%(compositor)s(%(particles)s) %(element)s unexpected, within %(parentElement)s"
msgstr " %(compositor)s ( %(particles)s) %(element)s неожиданный, внутри %(parentElement)s"

#: arelle\XmlValidateParticles.py:127
msgid "Particles of %(compositor)s have non-unique attribution of element %(element)s"
msgstr "Частицы %(compositor)s имеют уникальную атрибуцию элемента %(element)s"

#: arelle\XmlValidateParticles.py:140
msgid "Particle of %(compositor)s has non-unique xs:any preceding element %(element)s"
msgstr "Частица %(compositor)s имеет не-уникалный xs: любой предшествующий элемент %(element)s"

#: arelle\XmlValidateParticles.py:146
msgid "Particles of %(compositor)s have non-unique xs:any particle"
msgstr "Частицы %(compositor)s имеют не уникальные xs: любая частица"

#: arelle\XmlValidateSchema.py:108
msgid "Element attribute %(typeName)s value error: %(value)s, %(error)s"
msgstr "Атрибут элемента %(typeName)s s значение ошибка: %(value)s, %(error)s"
