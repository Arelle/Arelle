<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../testcase.xsl"?>
<testcase
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://xbrl.org/2005/conformance"
  name="NL.FR-NL-2.06"
  description="A CDATA end sequence (']]>') MAY NOT be used"
  outpath=''
  owner="support@arelle.org"
  xsi:schemaLocation="http://xbrl.org/2005/conformance https://www.xbrl.org/2005/conformance.xsd">
  <variation id="invalid-file" name="File with CDATA End Sequence">
    <description>
        The instance contains a CDATA end sequence.
    </description>
    <data>
        <instance readMeFirst="true">2-06-invalid-file.xbrl</instance>
    </data>
    <result>
      <error>NL.FR-NL-2.06</error>
    </result>
  </variation>
  <variation id="invalid-zip" name="Archive with CDATA End Sequence">
    <description>
      The archive instance contains a CDATA end sequence.
    </description>
    <data>
      <instance readMeFirst="true">2-06-invalid-zip.zip/2-06-invalid-file.xbrl</instance>
    </data>
    <result>
      <error>NL.FR-NL-2.06</error>
    </result>
  </variation>
</testcase>
