<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../testcase.xsl"?>
<testcase
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xbrl.org/2005/conformance"
        name="EDINET.EC5032E"
        description="EDINET.EC5032E: A manifest file for an IFRS submission must not define multiple instances."
        outpath=''
        owner="support@arelle.org"
        xsi:schemaLocation="http://xbrl.org/2005/conformance https://www.xbrl.org/2005/conformance.xsd">
    <variation id="invalid01" name="invalid01">
        <description>
            The IFRS submission defines multiple instances in the manifest.
            <!--valid05.zip with second (empty) "jpcrp040300_1" instance in manifest.-->
        </description>
        <data>
            <instance readMeFirst="true">invalid01.zip</instance>
        </data>
        <result>
            <error>FileNotLoadable</error> <!-- empty IXDS -->
            <error>EDINET.EC5032E</error>
            <warning>EDINET.EC8043W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8045W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8069W</warning> <!-- from base sample filing -->
        </result>
    </variation>
</testcase>
