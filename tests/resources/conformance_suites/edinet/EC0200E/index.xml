<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../testcase.xsl"?>
<testcase
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xbrl.org/2005/conformance"
        name="EDINET.EC0200E"
        description="EDINET.EC0200E: Report document filenames should not use characters other than those allowed (alphanumeric characters, '-' and '_')."
        outpath=''
        owner="support@arelle.org"
        xsi:schemaLocation="http://xbrl.org/2005/conformance https://www.xbrl.org/2005/conformance.xsd">
    <variation id="invalid01" name="invalid01">
        <description>
            Report document filename contains disallowed character.
            <!--valid09.zip with file duplicated and renamed with invalid space character.-->
        </description>
        <data>
            <instance readMeFirst="true">invalid01.zip</instance>
        </data>
        <result>
            <error num="1">EDINET.EC0200E</error>
            <error>EDINET.EC0352E</error> <!-- invalid chars in filename also means invalid filename -->
        </result>
    </variation>
</testcase>
