<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../testcase.xsl"?>
<testcase
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xbrl.org/2005/conformance"
        name="EDINET.EC3002E"
        description="EDINET.EC3002E: Table of contents numbers for table of contents entries must not be repeated within the same hierarchy."
        outpath=''
        owner="support@arelle.org"
        xsi:schemaLocation="http://xbrl.org/2005/conformance https://www.xbrl.org/2005/conformance.xsd">
    <variation id="invalid01" name="invalid01">
        <description>
            A TOC number is repeated within the same hierarchy.
            <!--valid08.zip with back-to-back repeating TOC numbers and a previous TOC number.-->
        </description>
        <data>
            <instance readMeFirst="true">invalid01.zip</instance>
        </data>
        <result>
            <error num="2">EDINET.EC3002E</error>
        </result>
    </variation>
</testcase>
