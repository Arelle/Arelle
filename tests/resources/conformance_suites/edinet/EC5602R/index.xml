<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../testcase.xsl"?>
<testcase
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://xbrl.org/2005/conformance"
        name="EDINET.EC5602R"
        description="EDINET.EC5602R: DEI value must match corresponding cover page value."
        outpath=''
        owner="support@arelle.org"
        xsi:schemaLocation="http://xbrl.org/2005/conformance https://www.xbrl.org/2005/conformance.xsd">
    <variation id="invalid01" name="invalid01">
        <description>
            DEI Japanese and English filer names do not match Cover Page values.
            <!--valid09.zip with FilerNameInJapaneseDEI "Ｇ株式会社" -> "ＧＧ株式会社" and FilerNameInEnglishDEI "G Corporation" -> "GG Corporation".-->
        </description>
        <data>
            <instance readMeFirst="true">invalid01.zip</instance>
        </data>
        <result>
            <warning num="2">EDINET.EC5602R</warning>
        </result>
    </variation>
    <variation id="invalid02" name="invalid02">
        <description>
            DEI Japanese fund name does not match Cover Page value.
            <!--valid12.zip with FundNameInJapaneseDEI "Ｆファンド" -> "ＦＦファンド".-->
        </description>
        <data>
            <instance readMeFirst="true">invalid02.zip</instance>
        </data>
        <result>
            <warning>EDINET.EC5602R</warning>
            <warning>EDINET.EC8038W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8042W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8045W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8049W</warning> <!-- from base sample filing -->
            <warning>EDINET.EC8050W</warning> <!-- from base sample filing -->
        </result>
    </variation>
</testcase>
