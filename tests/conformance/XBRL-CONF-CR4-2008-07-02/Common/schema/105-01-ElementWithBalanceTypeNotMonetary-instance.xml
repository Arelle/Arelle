<xbrl 
   xmlns="http://www.xbrl.org/2003/instance" 
   xmlns:link="http://www.xbrl.org/2003/linkbase" 
   xmlns:xlink="http://www.w3.org/1999/xlink" 
   xmlns:example="http://mycompany.com/xbrl/taxonomy/ext" 
   xmlns:iso4217="http://www.xbrl.org/2003/iso4217" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
   xsi:schemaLocation="http://mycompany.com/xbrl/taxonomy/ext 105-01-ElementWithBalanceTypeNotMonetary.xsd">
   <link:schemaRef xlink:href="105-01-ElementWithBalanceTypeNotMonetary.xsd" xlink:type="simple"/>

   <context id="CD1">
      <entity>
         <identifier scheme="www.example.com">example</identifier>
      </entity>
      <period>
         <startDate>2002-04-01</startDate>
         <endDate>2003-03-31</endDate>
      </period>
   </context>
   <unit id="U1">
      <measure>iso4217:USD</measure>
   </unit>

   <example:ItemWithBalance contextRef="CD1" unitRef="U1" decimals="0">5000</example:ItemWithBalance>



</xbrl>
