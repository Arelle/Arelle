<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:test="http://xbrl.org/formula/conformance/custom-test-link"
  xmlns:eg="http://xbrl.org/formula/conformance/example"  
  xmlns:xbrli="http://www.xbrl.org/2003/instance" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase 
      http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic 
      ../../../core_schemas/generic-link.xsd
  http://xbrl.org/formula/conformance/custom-test-link
      ../lib/custom-test-link.xsd
  ">
  
  <link:roleRef 
      roleURI="http://xbrl.org/formula/conformance/example/extlink1" 
      xlink:href="70017-arcLabel-concepts-v02.xsd#extlink1" 
      xlink:type="simple"/>
  <link:roleRef 
      roleURI="http://xbrl.org/formula/conformance/example/extlink2" 
      xlink:href="70017-arcLabel-concepts-v02.xsd#extlink2" 
      xlink:type="simple"/>
  <link:arcroleRef 
      arcroleURI="http://xbrl.org/formula/conformance/example/arcrole1" 
      xlink:href="70017-arcLabel-concepts-v02.xsd#arcrole1" 
      xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://xbrl.org/formula/conformance/example/extlink1">

    <test:loc xlink:type="locator" xlink:href="abc.xsd#a" xlink:label="a" />
    <test:loc xlink:type="locator" xlink:href="abc.xsd#b" xlink:label="b" />

  </generic:link>

  <!-- the purpose of this test is that the a and b labels are defined in a 
       different link from where they are referenced, thus being undefined
       in the following link -->

  <generic:link xlink:type="extended"
    xlink:role="http://xbrl.org/formula/conformance/example/extlink1">

    <generic:arc xlink:type="arc" xlink:from="a" xlink:to="b" order="1"
      xlink:arcrole="http://xbrl.org/formula/conformance/example/arcrole1"
      />      

  </generic:link>

</link:linkbase>




