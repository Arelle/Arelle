<testcase
xmlns="http://xbrl.org/2008/conformance"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
  http://xbrl.org/2008/conformance 
  ../../../infrastructure/test.xsd">
  <creator>
    <name>Herm Fischer</name>
    <email>fischer@markv.com</email>
  </creator>
  <number>70020</number>
  <name>Generic arc prohibition static analysis</name>
  <description>
    Test arc prohibition on generic arc element.
  </description>
  <reference specification="GENERIC-LINK" id="sec-arcrole"/>

  <variation id="V-01">
    <name>A generic arc has cycles, prohibition breaks the cycle of first linkbase so validation is clean.</name>
    <description>
      First linkbase testLB-v01 has cycle a->b->c->a; prohibiting linkbase
      (readMeFirst=true) prohibits c->a, so result expected is "valid".
    </description>
    <reference specification="GENERIC-LINK" id="sec-arcrole"/>
    <data>
      <schema>70020-arcProhibition-concepts-v01.xsd</schema>
      <linkbase>70020-arcProhibition-testLB-v01.xml</linkbase>
      <linkbase readMeFirst="true">70020-arcProhibition-testLB-prohibit-v01.xml</linkbase>
    </data>
    <result expected="valid"/>
  </variation>

</testcase>



