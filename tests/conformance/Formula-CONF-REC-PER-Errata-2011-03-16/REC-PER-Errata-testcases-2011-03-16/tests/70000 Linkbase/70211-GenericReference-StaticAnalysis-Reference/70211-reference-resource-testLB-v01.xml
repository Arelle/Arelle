<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:ref="http://xbrl.org/2008/reference"
  xmlns:test="http://xbrl.org/formula/conformance/custom-test-link"
  xmlns:eg="http://xbrl.org/formula/conformance/example"  
  xmlns:xbrli="http://www.xbrl.org/2003/instance" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase 
      http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic 
      ../../../core_schemas/generic-link.xsd
  http://xbrl.org/2008/reference 
      ../../../core_schemas/generic-reference.xsd
  http://xbrl.org/formula/conformance/custom-test-link
      ../lib/custom-test-link.xsd
  http://xbrl.org/formula/conformance/example 
          70211-reference-resource-concepts-v01.xsd
  ">
  
  <link:arcroleRef 
      arcroleURI="http://xbrl.org/arcrole/2008/element-reference" 
      xlink:href="../../../core_schemas/generic-reference.xsd#element-reference" 
      xlink:type="simple"/>

  <link:arcroleRef 
      arcroleURI="http://xbrl.org/arcrole/2008/test-arcrole-gen" 
      xlink:href="../lib/custom-test-link.xsd#test-arc-gen" 
      xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://www.xbrl.org/2003/role/link">

    <test:thing xlink:type="resource" xlink:label="a" id="a">
       Thing a.
    </test:thing>
    
    <test:thing xlink:type="resource" xlink:label="b" id="b">
       Thing b.
    </test:thing>
    
    <generic:arc id="arc_a_b" xlink:type="arc"
      xlink:from="a" xlink:to="b" order="1" priority="1"
      xlink:arcrole="http://xbrl.org/arcrole/2008/test-arcrole-gen" />

    <!-- references for the preceeding linkbase things -->
    <ref:reference xlink:type="resource" xlink:label="thing_a_ref" 
      xlink:role="http://www.xbrl.org/2003/role/reference" >
        <eg:PartA>AAA for a</eg:PartA>
        <eg:PartB>BBB for a</eg:PartB>
        <eg:PartC>CCC for a</eg:PartC>
    </ref:reference>

    <ref:reference xlink:type="resource" xlink:label="thing_b_ref" 
      xlink:role="http://www.xbrl.org/2003/role/reference" >
        <eg:PartA>AAA for b</eg:PartA>
        <eg:PartB>BBB for b</eg:PartB>
        <eg:PartC>CCC for b</eg:PartC>
    </ref:reference>

    <ref:reference xlink:type="resource" xlink:label="arc_a_b_ref" 
      xlink:role="http://www.xbrl.org/2003/role/reference" >
        <eg:PartA>AAA for arc</eg:PartA>
        <eg:PartB>BBB for arc</eg:PartB>
        <eg:PartC>CCC for arc</eg:PartC>
    </ref:reference>

    <ref:reference xlink:type="resource" xlink:label="xsd_attrib_ref" 
      xlink:role="http://www.xbrl.org/2003/role/reference" >
        <eg:PartA>AAA for attrib in xsd</eg:PartA>
        <eg:PartB>BBB for attrib in xsd</eg:PartB>
        <eg:PartC>CCC for attrib in xsd</eg:PartC>
    </ref:reference>

    <!-- arcs for references, resources pretty simple -->
    <generic:arc xlink:type="arc" 
      xlink:arcrole="http://xbrl.org/arcrole/2008/element-reference"
      xlink:from="a" xlink:to="thing_a_ref" order="1.0"/>

    <generic:arc xlink:type="arc" 
      xlink:arcrole="http://xbrl.org/arcrole/2008/element-reference"
      xlink:from="b" xlink:to="thing_b_ref" order="1.0"/>

    <!-- arcs for labels, on an arc requires a (nondiscovering) loc -->
    <test:loc xlink:type="locator" 
      xlink:href="#arc_a_b" 
      xlink:label="arc_a_b_loc" />
    <generic:arc xlink:type="arc" 
      xlink:arcrole="http://xbrl.org/arcrole/2008/element-reference"
      xlink:from="arc_a_b_loc" xlink:to="arc_a_b_ref" order="1.0"/>

    <!-- arcs for labels, off to xsd requires a (nondiscovering) loc -->
    <test:loc xlink:type="locator" 
      xlink:href="70211-reference-resource-concepts-v01.xsd#anAttribute" 
      xlink:label="xsd_attrib_loc" />
    <generic:arc xlink:type="arc" 
      xlink:arcrole="http://xbrl.org/arcrole/2008/element-reference"
      xlink:from="xsd_attrib_loc" xlink:to="xsd_attrib_ref" order="1.0"/>

  </generic:link>

</link:linkbase>




