  <creator>
    <name>Herm Fischer</name>
    <email>fischer@markv.com</email>
  </creator>
  <number>12030</number>
  <name>Concept rules processing examples</name>
  <description>
    Tests of formula concept rules processing
  </description>
  <reference specification="FORMULA" id="sec-concept-rules"/>

  <variation id="V-01">
    <name>Test of dynamic concept QName expressions</name>
    <description>Tests that the output concept is that produced by a QName expression, which in this case reverses the fact item concept name-value matchup sequence, from that of the source input.  E.g., the value, context, and unit of m1 are output as m4, m2 as m3, m3 as m2, and m4 as m1.</description>
    <reference specification="FORMULA" id="example-dynamic-concept-rules"/>
    <data>
      <schema>12030-concept-tests.xsd</schema>
      <linkbase>12030-dynamic-concept-formula.xml</linkbase>
      <instance readMeFirst="true">12030-dynamic-concept-instance.xml</instance>
    </data>
    <result>
      <instance>12030-dynamic-concept-result.xml</instance>
    </result>
  </variation>  

</testcase>


