    type="xbrli:monetaryItemType" abstract="false" 
    nillable="true" />

  <element name="liabilities" id="eg_liabilities" 
    xbrli:periodType="instant" substitutionGroup="xbrli:item" 
    type="xbrli:monetaryItemType" abstract="false" 
    nillable="true" />

  <element name="auditExpenses" id="eg_auditExpenses" 
