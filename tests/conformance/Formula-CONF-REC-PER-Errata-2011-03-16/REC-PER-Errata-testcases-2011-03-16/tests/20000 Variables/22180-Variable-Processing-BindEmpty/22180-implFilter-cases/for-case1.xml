   <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set"
                    xlink:href="../../../../core_schemas/variable.xsd#variable-set"
                    xlink:type="simple"/>
   <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set-filter"
                    xlink:href="../../../../core_schemas/variable.xsd#variable-set-filter"
                    xlink:type="simple"/>
   <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set-precondition"
