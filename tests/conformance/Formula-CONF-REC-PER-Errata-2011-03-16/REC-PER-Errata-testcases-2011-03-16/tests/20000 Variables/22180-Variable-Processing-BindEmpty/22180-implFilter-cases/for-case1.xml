   <generic:link xlink:type="extended" xlink:role="http://www.xbrl.org/2008/role/link">
      <formula:formula xlink:label="for_case1" value="string-join(($a,$b,$c), '.')" id="for_case1"
                       aspectModel="dimensional"
                       implicitFiltering="true"
                       source="formula:uncovered"
                       xlink:type="resource">
         <formula:aspects>
            <formula:concept>
               <formula:qname>s:R</formula:qname>
            </formula:concept>
         </formula:aspects>
      </formula:formula>
      <variable:variableArc name="a" xlink:from="for_case1" xlink:to="var_s_A"
                            xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set"
                            xlink:type="arc"
                            order="1"/>
      <variable:variableArc name="b" xlink:from="for_case1" xlink:to="var_s_BDDim_X"
                            xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set"
                            xlink:type="arc"
                            order="2"/>
      <variable:variableArc name="c" xlink:from="for_case1" xlink:to="var_s_C"
                            xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set"
                            xlink:type="arc"
                            order="3"/>
      <variable:factVariable xlink:label="var_s_A" bindAsSequence="false" id="var_s_A" xlink:type="resource"/>
      <variable:variableFilterArc complement="false" xlink:from="var_s_A" xlink:to="fil_s_A" cover="true"
                                  xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter"
                                  xlink:type="arc"
                                  order="1"/>
      <cf:conceptName xlink:label="fil_s_A" id="fil_s_A" xlink:type="resource">
         <cf:concept>
            <cf:qname>s:A</cf:qname>
         </cf:concept>
      </cf:conceptName>
      <variable:factVariable xlink:label="var_s_BDDim_X" bindAsSequence="false" id="var_s_BDDim_X"
                             xlink:type="resource"
                             fallbackValue="''"/>
      <variable:variableFilterArc complement="false" xlink:from="var_s_BDDim_X" xlink:to="fil_s_B" cover="true"
                                  xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter"
                                  xlink:type="arc"
                                  order="1"/>
      <variable:variableFilterArc complement="false" xlink:from="var_s_BDDim_X" xlink:to="fil_DDim_X"
                                  cover="true"
                                  xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter"
                                  xlink:type="arc"
                                  order="2"/>
      <cf:conceptName xlink:label="fil_s_B" id="fil_s_B" xlink:type="resource">
         <cf:concept>
            <cf:qname>s:B</cf:qname>
         </cf:concept>
      </cf:conceptName>
      <df:explicitDimension xlink:label="fil_DDim_X" id="fil_DDim_X" xlink:type="resource">
         <df:dimension>
            <df:qname>s:D</df:qname>
         </df:dimension>
         <df:member>
            <df:qname>s:X</df:qname>
         </df:member>
      </df:explicitDimension>
      <variable:factVariable xlink:label="var_s_C" bindAsSequence="false" id="var_s_C" xlink:type="resource" fallbackValue="''"/>
      <variable:variableFilterArc complement="false" xlink:from="var_s_C" xlink:to="fil_s_C" cover="true"
                                  xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter"
                                  xlink:type="arc"
                                  order="1"/>
      <cf:conceptName xlink:label="fil_s_C" id="fil_s_C" xlink:type="resource">
         <cf:concept>
            <cf:qname>s:C</cf:qname>
         </cf:concept>
      </cf:conceptName>
