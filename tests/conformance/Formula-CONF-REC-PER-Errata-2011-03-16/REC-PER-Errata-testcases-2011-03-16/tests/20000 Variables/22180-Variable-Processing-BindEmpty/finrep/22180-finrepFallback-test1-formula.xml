<?xml version="1.0" encoding="utf-8"?>
<link:linkbase 
  xmlns:link="http://www.xbrl.org/2003/linkbase" 
  xmlns:generic="http://xbrl.org/2008/generic" 
  xmlns:formula="http://xbrl.org/2008/formula" 
  xmlns:variable="http://xbrl.org/2008/variable" 
  xmlns:ea="http://xbrl.org/2008/assertion/existence"
  xmlns:va="http://xbrl.org/2008/assertion/value"
  xmlns:cf="http://xbrl.org/2008/filter/concept" 
  xmlns:df="http://xbrl.org/2008/filter/dimension" 
  xmlns:ef="http://xbrl.org/2008/filter/entity" 
  xmlns:gf="http://xbrl.org/2008/filter/general" 
  xmlns:pf="http://xbrl.org/2008/filter/period" 
  xmlns:tf="http://xbrl.org/2008/filter/tuple" 
  xmlns:uf="http://xbrl.org/2008/filter/unit" 
  xmlns:vf="http://xbrl.org/2008/filter/value" 
  xmlns:label="http://xbrl.org/2008/label" 
  xmlns:eg="http://xbrl.org/formula/conformance/example" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xfi="http://www.xbrl.org/2008/function/instance"
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xmlns:xbrli="http://www.xbrl.org/2003/instance"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase
    http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic
    ../../../../core_schemas/generic-link.xsd 
  http://xbrl.org/2008/assertion/existence 
    ../../../../core_schemas/existence-assertion.xsd
  http://xbrl.org/2008/assertion/value 
    ../../../../core_schemas/value-assertion.xsd
  http://xbrl.org/2008/formula
    ../../../../core_schemas/formula.xsd
  http://xbrl.org/2008/variable
    ../../../../core_schemas/variable.xsd
  http://xbrl.org/2008/filter/concept
    ../../../../core_schemas/concept-filter.xsd
  http://xbrl.org/2008/filter/period 
    ../../../../core_schemas/period-filter.xsd
  http://xbrl.org/2008/filter/dimension
    ../../../../core_schemas/dimension-filter.xsd
  http://xbrl.org/2008/label
    ../../../../core_schemas/generic-label.xsd
  http://xbrl.org/formula/conformance/example
    22180-finrepFallback-test1.xsd"
  >
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../../core_schemas/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../../core_schemas/variable.xsd#variable-filter" xlink:type="simple"/>
  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../../core_schemas/generic-link.xsd#standard-link-role" xlink:type="simple"/>

  <generic:link xlink:type="extended" xlink:role="http://www.xbrl.org/2008/role/link">

    <ea:existenceAssertion xlink:type="resource"
      xlink:label="assertion1"
      id="existence-assertion"
      aspectModel="dimensional" implicitFiltering="true"/>

    <!-- provide assertion for radius according to parameter -->
    <va:valueAssertion xlink:type="resource"
      xlink:label="assertion1"
      id="value-assertion"
      test="abs($v1 - $v2 - $v3 - $v4) lt 0.01"
      aspectModel="dimensional" implicitFiltering="true" />

    <!--  Arc from the assertions to the variables it depends on -->
    <variable:factVariable xlink:type="resource" 
      fallbackValue="0"
      xlink:label="factvar1" bindAsSequence="false" />
    <cf:conceptName xlink:type="resource" xlink:label="filters1">
      <cf:concept>
        <cf:qname>eg:n1</cf:qname>
      </cf:concept>
    </cf:conceptName>
    <df:explicitDimension xlink:type="resource" xlink:label="filters1">
      <df:dimension>
        <df:qname>eg:dim1</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim1Mem1</df:qname>
      </df:member>
    </df:explicitDimension>

    <variable:factVariable xlink:type="resource" 
      fallbackValue="0"
      xlink:label="factvar2" bindAsSequence="false" />
    <cf:conceptName xlink:type="resource" xlink:label="filters2">
      <cf:concept>
        <cf:qname>eg:n2</cf:qname>
      </cf:concept>
    </cf:conceptName>
    <df:explicitDimension xlink:type="resource" xlink:label="filters2">
      <df:dimension>
        <df:qname>eg:dim1</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim1Mem1</df:qname>
      </df:member>
    </df:explicitDimension>
    <df:explicitDimension xlink:type="resource" xlink:label="filters2">
      <df:dimension>
        <df:qname>eg:dim2</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim2Mem1</df:qname>
      </df:member>
    </df:explicitDimension>

    <variable:factVariable xlink:type="resource" 
      fallbackValue="0"
      xlink:label="factvar3" bindAsSequence="false" />
    <cf:conceptName xlink:type="resource" xlink:label="filters3">
      <cf:concept>
        <cf:qname>eg:n3</cf:qname>
      </cf:concept>
    </cf:conceptName>
    <df:explicitDimension xlink:type="resource" xlink:label="filters3">
      <df:dimension>
        <df:qname>eg:dim1</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim1Mem1</df:qname>
      </df:member>
    </df:explicitDimension>
    <df:explicitDimension xlink:type="resource" xlink:label="filters3">
      <df:dimension>
        <df:qname>eg:dim2</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim2Mem2</df:qname>
      </df:member>
    </df:explicitDimension>

    <variable:factVariable xlink:type="resource" 
      fallbackValue="0"
      xlink:label="factvar4" bindAsSequence="false" />
    <cf:conceptName xlink:type="resource" xlink:label="filters4">
      <cf:concept>
        <cf:qname>eg:n4</cf:qname>
      </cf:concept>
    </cf:conceptName>
    <df:explicitDimension xlink:type="resource" xlink:label="filters4">
      <df:dimension>
        <df:qname>eg:dim1</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim1Mem1</df:qname>
      </df:member>
    </df:explicitDimension>
    <df:explicitDimension xlink:type="resource" xlink:label="filters4">
      <df:dimension>
        <df:qname>eg:dim2</df:qname>
      </df:dimension>
      <df:member>
        <df:qname>eg:dim2Mem3</df:qname>
      </df:member>
    </df:explicitDimension>

    <variable:variableArc xlink:type="arc" xlink:to="factvar1" xlink:from="assertion1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="1" 
      name="v1" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filters1" xlink:from="factvar1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="true" />
    <variable:variableArc xlink:type="arc" xlink:to="factvar2" xlink:from="assertion1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="2" 
      name="v2" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filters2" xlink:from="factvar2" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="true" />

    <variable:variableArc xlink:type="arc" xlink:to="factvar3" xlink:from="assertion1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="2" 
      name="v3" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filters3" xlink:from="factvar3" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="true" />

    <variable:variableArc xlink:type="arc" xlink:to="factvar4" xlink:from="assertion1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="2" 
      name="v4" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="filters4" xlink:from="factvar4" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="true" />

  </generic:link>
</link:linkbase>






