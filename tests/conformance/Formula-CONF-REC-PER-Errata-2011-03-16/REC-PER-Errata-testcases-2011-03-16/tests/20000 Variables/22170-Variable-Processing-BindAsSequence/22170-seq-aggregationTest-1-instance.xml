<xbrl 
xmlns="http://www.xbrl.org/2003/instance" 
xmlns:concept="http://xbrl.org/formula/conformance/example" 
xmlns:link="http://www.xbrl.org/2003/linkbase" 
xmlns:xlink="http://www.w3.org/1999/xlink" 
xmlns:iso4217="http://www.xbrl.org/2003/iso4217" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:xbrldi="http://xbrl.org/2006/xbrldi"
xsi:schemaLocation="http://xbrl.org/2006/xbrldi http://www.xbrl.org/2006/xbrldi-2006.xsd"
>
  
  <link:schemaRef 
    xlink:type="simple" 
    xlink:href="22170-sequence-dim-tests.xsd"/>

  <link:linkbaseRef 
    xlink:type="simple" 
    xlink:arcrole="http://www.w3.org/1999/xlink/properties/linkbase" 
    xlink:href="22170-seq-aggregationTest-1-formula.xml"/>
  
  <context id="context-earth">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Earth</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-mars">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Mars</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-jupiter">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Jupiter</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-saturn">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Saturn</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-uranus">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Uranus</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-neptune">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Neptune</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-all-planets">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:AllPlanets</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <unit id="u-moon-count">
    <measure>pure</measure>
  </unit>
  

    <!-- real naturally-formed moons -->
    <concept:MoonCount contextRef="context-earth" unitRef="u-moon-count" decimals="0">1</concept:MoonCount>
    <concept:MoonCount contextRef="context-mars" unitRef="u-moon-count" decimals="0">2</concept:MoonCount>
    <concept:MoonCount contextRef="context-jupiter" unitRef="u-moon-count" decimals="0">16</concept:MoonCount>
    <concept:MoonCount contextRef="context-saturn" unitRef="u-moon-count" decimals="0">18</concept:MoonCount>
    <concept:MoonCount contextRef="context-neptune" unitRef="u-moon-count" decimals="0">27</concept:MoonCount>
    <concept:MoonCount contextRef="context-uranus" unitRef="u-moon-count" decimals="0">8</concept:MoonCount>

    <!-- manufactured satellites -->
    <concept:SatelliteCount contextRef="context-earth" unitRef="u-moon-count" decimals="0">8000</concept:SatelliteCount>
    <concept:SatelliteCount contextRef="context-mars" unitRef="u-moon-count" decimals="0">4</concept:SatelliteCount>

    <!-- totals so we can use this same instance document as consistency assertion test -->
    <concept:MoonCount contextRef="context-all-planets" unitRef="u-moon-count" decimals="0">72</concept:MoonCount>
    <concept:SatelliteCount contextRef="context-all-planets" unitRef="u-moon-count" decimals="0">8004</concept:SatelliteCount>


</xbrl>






