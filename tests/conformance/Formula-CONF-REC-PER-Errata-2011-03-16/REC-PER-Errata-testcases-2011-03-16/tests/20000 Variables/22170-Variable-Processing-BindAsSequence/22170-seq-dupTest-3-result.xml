<xbrl 
xmlns="http://www.xbrl.org/2003/instance" 
xmlns:concept="http://xbrl.org/formula/conformance/example" 
xmlns:link="http://www.xbrl.org/2003/linkbase" 
xmlns:xlink="http://www.w3.org/1999/xlink" 
xmlns:iso4217="http://www.xbrl.org/2003/iso4217" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:xbrldi="http://xbrl.org/2006/xbrldi"
xsi:schemaLocation="http://xbrl.org/2006/xbrldi http://www.xbrl.org/2006/xbrldi-2006.xsd"
>
  
  <link:schemaRef 
    xlink:type="simple" 
    xlink:href="22170-sequence-dim-tests.xsd"/>

  <context id="context-earth">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Earth</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-mars">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Mars</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

   <context id="context-jupiter">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Jupiter</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-saturn">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Saturn</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-uranus">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Uranus</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <context id="context-neptune">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
      <segment>
        <xbrldi:explicitMember dimension="concept:MoonDim">concept:Neptune</xbrldi:explicitMember>
      </segment>
    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <unit id="u-moon-count">
    <measure>pure</measure>
  </unit>
  


    <!-- Earth moon fails precondition, not tested 
    <concept:Moon contextRef="context-earth">no evaluation here</concept:Moon>
    -->
    <!-- this count passes precondition and is duplicated, passes assertion -->
    <concept:MoonCount contextRef="context-earth" unitRef="u-moon-count" precision='0'>1</concept:MoonCount>

    <!-- deimos moon is duplicated here, passes precond with Phobos and fail assertion 
         because some (Deimos) moon is duplicated -->
    <concept:Moon contextRef="context-mars">yes</concept:Moon>

    <!-- no duplicate, not evaluated
    <concept:MoonCount contextRef="context-mars" unitRef="u-moon-count" precision='0'>no evaluation</concept:MoonCount>
    -->

    <!-- passes precondition but not duplicated -->
    <concept:Moon contextRef="context-jupiter">no</concept:Moon>

    <!-- passes precondition but not duplicated -->
    <concept:Moon contextRef="context-saturn">no</concept:Moon>

    <!-- passes precondition but not duplicated -->
    <concept:Moon contextRef="context-neptune">no</concept:Moon>

    <!-- passes precondition but not duplicated -->
    <concept:Moon contextRef="context-uranus">no</concept:Moon>

</xbrl>

