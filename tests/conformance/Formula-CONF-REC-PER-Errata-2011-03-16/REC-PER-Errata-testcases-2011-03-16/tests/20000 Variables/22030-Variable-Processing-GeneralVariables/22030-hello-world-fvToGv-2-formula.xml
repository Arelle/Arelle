<?xml version="1.0" encoding="utf-8"?>
<link:linkbase 
  xmlns:link="http://www.xbrl.org/2003/linkbase" 
  xmlns:generic="http://xbrl.org/2008/generic" 
  xmlns:formula="http://xbrl.org/2008/formula" 
  xmlns:variable="http://xbrl.org/2008/variable" 
  xmlns:cf="http://xbrl.org/2008/filter/concept" 
  xmlns:df="http://xbrl.org/2008/filter/dimension" 
  xmlns:ef="http://xbrl.org/2008/filter/entity" 
  xmlns:gf="http://xbrl.org/2008/filter/general" 
  xmlns:pf="http://xbrl.org/2008/filter/period" 
  xmlns:tf="http://xbrl.org/2008/filter/tuple" 
  xmlns:uf="http://xbrl.org/2008/filter/unit" 
  xmlns:vf="http://xbrl.org/2008/filter/value" 
  xmlns:label="http://xbrl.org/2008/label" 
  xmlns:eg="http://xbrl.org/formula/conformance/example" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase
    http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic
    ../../../core_schemas/generic-link.xsd 
  http://xbrl.org/2008/formula
    ../../../core_schemas/formula.xsd
  http://xbrl.org/2008/variable
    ../../../core_schemas/variable.xsd
  http://xbrl.org/2008/filter/concept
    ../../../core_schemas/concept-filter.xsd
  http://xbrl.org/2008/filter/general
    ../../../core_schemas/general-filter.xsd
  http://xbrl.org/2008/label
    ../../../core_schemas/generic-label.xsd
  http://xbrl.org/formula/conformance/example
    22030-genVar-tests.xsd"
  >
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/variable.xsd#variable-filter" xlink:type="simple"/>  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/generic-link.xsd#standard-link-role" xlink:type="simple"/>

  <generic:link xlink:type="extended" xlink:role="http://www.xbrl.org/2008/role/link">
    <formula:formula xlink:type="resource" xlink:label="formula1" 
      value="$g1" source="c2" aspectModel="dimensional" implicitFiltering="false" />

    <!-- $c1 picks up value "hello" from instance -->      
    <variable:factVariable xlink:type="resource" xlink:label="factvar1" bindAsSequence="true"  />
    <cf:conceptName xlink:type="resource" xlink:label="namefilter1">
      <cf:concept>
        <cf:qname>eg:c1</cf:qname>
      </cf:concept>
    </cf:conceptName>

    <!-- $c2 binds to item with same value as $c1 after $g2 depends on $c1 -->
    <variable:factVariable xlink:type="resource" xlink:label="factvar2" bindAsSequence="false"  />
    <gf:general xlink:type="resource" xlink:label="genfilter1"
      test=". eq $g2"/>

    <!-- $g2 references $c1 in a for construct -->
    <variable:generalVariable xlink:type="resource" xlink:label="genvar2" 
      bindAsSequence="false"
      select="
        distinct-values(
          for $foo in $c1,
              $bar in $foo
                 return $bar)
             "
     />

    <!-- $g1 concatenates " world" to $c1 -->
    <variable:generalVariable xlink:type="resource" xlink:label="genvar1" 
      select="concat($g2,' ',$w1)" bindAsSequence="false"/>

    <!-- $w1 depends on $w1 -->
    <variable:generalVariable xlink:type="resource" xlink:label="genvarw1" 
      select="for $foo in $w2 return $foo" bindAsSequence="false"/>

    <!-- $w2 -->
    <variable:generalVariable xlink:type="resource" xlink:label="genvarw2" 
      select="( (), ('world'), () )" bindAsSequence="true"/>

   

    <!-- variables are presented in reverse of dependency order -->
    <!-- g1 depends on g2, so g2 should be resolved before g1 -->
    <variable:variableArc xlink:type="arc" xlink:to="genvar1" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="1" 
      name="g1" />
    <!-- g2 depends on c1, so c1 should be resolved before g1 -->
    <variable:variableArc xlink:type="arc" xlink:to="genvar2" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="1" 
      name="g2" />
    <!-- w1 -->
    <variable:variableArc xlink:type="arc" xlink:to="genvarw1" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="2" 
      name="w1" />
    <!-- w2 -->
    <variable:variableArc xlink:type="arc" xlink:to="genvarw2" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="3" 
      name="w2" />
    <!-- c1 -->
    <variable:variableArc xlink:type="arc" xlink:to="factvar1" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="4" 
      name="c1" />
    <variable:variableArc xlink:type="arc" xlink:to="factvar2" xlink:from="formula1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" order="5" 
      name="c2" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="namefilter1" xlink:from="factvar1" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="false" />
    <variable:variableFilterArc xlink:type="arc" xlink:to="genfilter1" xlink:from="factvar2" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" order="1" complement="false" cover="false" />
  </generic:link>
</link:linkbase>

