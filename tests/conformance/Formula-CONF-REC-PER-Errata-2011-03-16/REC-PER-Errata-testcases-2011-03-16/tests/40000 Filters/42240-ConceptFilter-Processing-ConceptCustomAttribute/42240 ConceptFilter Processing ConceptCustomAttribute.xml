<?xml-stylesheet type="text/xsl" href="../../../infrastructure/test.xsl"?>
<testcase
xmlns="http://xbrl.org/2008/conformance"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
  http://xbrl.org/2008/conformance 
  ../../../infrastructure/test.xsd">
  <creator>
    <name>Takahide Muramoto, Hitoshi Okumura, Shigehisa Miyano</name>
    <email>taka.muramoto@jp.fujitsu.com, okmr@jp.fujitsu.com, s-miyano@jp.fujitsu.com</email>
  </creator>
  <number>42240</number>
  <name>Concept Custom-Attribute Filter Processing</name>
  <description>
    Test concept custom-attribute filter processing
  </description>
  <reference specification="CONCEPT-FILTER" id="sec-concept-custom-attribute-filter"/>

  <variation id="V-01">
    <name>Test custom attribute existence filter.</name>
    <description>The fact's concept must have an attribute "eg:customString" on it.</description>
    <reference specification="CONCEPT-FILTER" id="example-concept-custom-attribute-filter"/>
    <data>
      <schema>42240-customAttributeFilter-concepts.xsd</schema>
      <linkbase>42240-customAttributeFilter-qname-formula.xml</linkbase>
      <instance readMeFirst="true">42240-customAttributeFilter-qname-instance.xml</instance>
    </data>
    <result>
      <instance>42240-customAttributeFilter-qname-result.xml</instance>
    </result>
  </variation>

  <variation id="V-02">
    <name>Test custom string attribute value filter.</name>
    <description>The fact's concept must have an attribute "eg:customString" on it with string content: confidential.</description>
    <reference specification="CONCEPT-FILTER" id="example-concept-custom-attribute-filter"/>
    <data>
      <schema>42240-customAttributeFilter-concepts.xsd</schema>
      <linkbase>42240-customAttributeFilter-qname-strval-formula.xml</linkbase>
      <instance readMeFirst="true">42240-customAttributeFilter-qname-strval-instance.xml</instance>
    </data>
    <result>
      <instance>42240-customAttributeFilter-qname-strval-result.xml</instance>
    </result>
  </variation>

  <variation id="V-03">
    <name>Test custom boolean attribute value filter.</name>
    <description>The fact's concept must have an attribute "eg:customBoolean" on it with boolean content: false.</description>
    <reference specification="CONCEPT-FILTER" id="example-concept-custom-attribute-filter"/>
    <data>
      <schema>42240-customAttributeFilter-concepts.xsd</schema>
      <linkbase>42240-customAttributeFilter-qnameExpression-boolval-formula.xml</linkbase>
      <instance readMeFirst="true">42240-customAttributeFilter-qnameExpression-boolval-instance.xml</instance>
    </data>
    <result>
      <instance>42240-customAttributeFilter-qnameExpression-boolval-result.xml</instance>
    </result>
  </variation>

</testcase>

