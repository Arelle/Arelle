<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:formula="http://xbrl.org/2008/formula"
  xmlns:validation="http://xbrl.org/2008/validation"
  xmlns:variable="http://xbrl.org/2008/variable"
  xmlns:cf="http://xbrl.org/2008/filter/concept"
  xmlns:gf="http://xbrl.org/2008/filter/general"
  xmlns:bf="http://xbrl.org/2008/filter/boolean"
  xmlns:ef="http://xbrl.org/2008/filter/entity"
  xmlns:pf="http://xbrl.org/2008/filter/period"
  xmlns:tf="http://xbrl.org/2008/filter/tuple"
  xmlns:label="http://xbrl.org/2008/label"
  xmlns:xfi="http://www.xbrl.org/2008/function/instance" 
  xmlns:v="http://xbrl.org/2008/formula/conformance/variable"
  xmlns:eg="http://xbrl.org/formula/conformance/example"  
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:xbrli="http://www.xbrl.org/2003/instance" 
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic ../../../core_schemas/generic-link.xsd
  http://xbrl.org/2008/formula ../../../core_schemas/formula.xsd
  http://xbrl.org/2008/variable ../../../core_schemas/variable.xsd
  http://xbrl.org/2008/filter/concept ../../../core_schemas/concept-filter.xsd
  http://xbrl.org/2008/filter/boolean ../../../core_schemas/boolean-filter.xsd
  http://xbrl.org/2008/filter/entity ../../../core_schemas/entity-filter.xsd
  http://xbrl.org/2008/filter/period ../../../core_schemas/period-filter.xsd
  http://xbrl.org/2008/filter/tuple ../../../core_schemas/tuple-filter.xsd
  http://xbrl.org/2008/label ../../../core_schemas/generic-label.xsd
  ">
  
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/variable.xsd#variable-filter" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/boolean-filter" xlink:href="../../../core_schemas/boolean-filter.xsd#boolean-filter" xlink:type="simple"/>  <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/generic-link.xsd#standard-link-role" xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://www.xbrl.org/2008/role/link">


    <formula:formula xlink:type="resource" xlink:label="formula"
      value="$v:b * $v:c_or_d"
      source="v:b"
      aspectModel="dimensional" implicitFiltering="true">
      <formula:decimals>0</formula:decimals>
      <formula:aspects>
        <formula:concept>
          <formula:qname>eg:a</formula:qname>
        </formula:concept>
      </formula:aspects>
    </formula:formula>

    <!--  Fact variables.  Note that no general variables are used in this formula. -->
    <variable:factVariable xlink:type="resource" xlink:label="factVarB"  bindAsSequence="false"/>
    <variable:factVariable xlink:type="resource" xlink:label="factVarC"  bindAsSequence="false"/>
    
    <!--  Concept filters for fact variable B -->
    <cf:conceptName xlink:type="resource" xlink:label="nameFilterB">
      <cf:concept>
        <cf:qname>eg:b</cf:qname>
      </cf:concept>
    </cf:conceptName> 

    <!-- top level and filter for tandem boolean sub-filters -->
    <bf:andFilter xlink:type="resource" xlink:label="topTandemFilter"/>

    <!-- and subfilter for fact variable location and period -->
    <bf:andFilter xlink:type="resource" xlink:label="andFilter"/>

    <!-- and subfilter subfilters -->
    <tf:parentFilter xlink:type="resource" xlink:label="rootFilter">
      <tf:parent><tf:qname>xbrli:xbrl</tf:qname></tf:parent>
    </tf:parentFilter>
    <pf:forever xlink:type="resource" xlink:label="foreverFilter" /> 

    <!-- or subfilter for fact variable name C and scheme_02 -->
    <bf:orFilter xlink:type="resource" xlink:label="orNameFilter"/>

    <!-- and subfilter subfilters -->
    <cf:conceptName xlink:type="resource" xlink:label="nameFilterC">
      <cf:concept>
        <cf:qname>eg:c</cf:qname>
      </cf:concept>
    </cf:conceptName> 
    <cf:conceptName xlink:type="resource" xlink:label="nameFilterD">
      <cf:concept>
        <cf:qname>eg:d</cf:qname>
      </cf:concept>
    </cf:conceptName> 

    <!--  Arc from the formula to the variables it depends on -->
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:b" xlink:from="formula" xlink:to="factVarB" order="1.0"/>
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set" 
      name="v:c_or_d" xlink:from="formula" xlink:to="factVarC" order="1.0"/>

    <!-- Arcs from topTandemFilter to sub-boolean filters -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter" 
      complement="false" cover="false"
      xlink:to="andFilter" xlink:from="topTandemFilter" order="1" />
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter" 
      complement="false" cover="true"
      xlink:to="orNameFilter" xlink:from="topTandemFilter" order="2" />

    <!--  Arcs from variables to their concept filters -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarB" xlink:to="nameFilterB" order="1.0"/>
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="factVarC" xlink:to="topTandemFilter" order="1.0"/>

    <!-- Arcs from sub-or filter to its sub filters -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter" 
      complement="false" cover="true"
      xlink:to="nameFilterC" xlink:from="orNameFilter" order="1" />
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter"
      complement="false" cover="true"
      xlink:to="nameFilterD" xlink:from="orNameFilter"  order="2" />

    <!-- Arcs from sub-and filter to its sub filters -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter" 
      complement="false" cover="false"
      xlink:to="rootFilter" xlink:from="andFilter" order="1" />
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/boolean-filter"
      complement="false" cover="false"
      xlink:to="foreverFilter" xlink:from="andFilter" order="2" />

  </generic:link>

</link:linkbase>


