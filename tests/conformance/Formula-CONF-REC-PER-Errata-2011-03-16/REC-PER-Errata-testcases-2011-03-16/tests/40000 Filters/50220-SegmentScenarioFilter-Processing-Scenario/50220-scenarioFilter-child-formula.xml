<link:linkbase
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:link="http://www.xbrl.org/2003/linkbase"
  xmlns:generic="http://xbrl.org/2008/generic"
  xmlns:formula="http://xbrl.org/2008/formula"
  xmlns:variable="http://xbrl.org/2008/variable"
  xmlns:ssf="http://xbrl.org/2008/filter/segment-scenario"
  xmlns:label="http://xbrl.org/2008/label"
  xmlns:xfi="http://www.xbrl.org/2008/function/instance"
  xmlns:v="http://xbrl.org/2008/formula/conformance/variable"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:eg="http://xbrl.org/formula/conformance/example"
  xsi:schemaLocation="
  http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
  http://xbrl.org/2008/generic ../../../core_schemas/generic-link.xsd
  http://xbrl.org/2008/formula ../../../core_schemas/formula.xsd
  http://xbrl.org/2008/variable ../../../core_schemas/variable.xsd
  http://xbrl.org/2008/filter/segment-scenario ../../../core_schemas/segment-scenario-filter.xsd
  http://xbrl.org/2008/label ../../../core_schemas/generic-label.xsd
  http://xbrl.org/formula/conformance/example 50220-scenarioFilter-concepts.xsd
  ">
  
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/element-label" xlink:href="../../../core_schemas/generic-label.xsd#element-label" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-set" xlink:href="../../../core_schemas/variable.xsd#variable-set" xlink:type="simple"/>
  <link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2008/variable-filter" xlink:href="../../../core_schemas/variable.xsd#variable-filter" xlink:type="simple"/>
    <link:roleRef roleURI="http://www.xbrl.org/2008/role/link" xlink:href="../../../core_schemas/generic-link.xsd#standard-link-role" xlink:type="simple"/>
  <link:roleRef roleURI="http://www.xbrl.org/2008/role/label" xlink:href="../../../core_schemas/generic-label.xsd#standard-label" xlink:type="simple"/>

  <generic:link xlink:type="extended"
    xlink:role="http://www.xbrl.org/2008/role/link">

    <formula:formula xlink:type="resource" xlink:label="formula"
      aspectModel="non-dimensional" implicitFiltering="true" 
      value="$v:v"
      source="v:v" >
      <formula:decimals>0</formula:decimals>
    </formula:formula>

    <!--  Fact variables -->
    <variable:factVariable xlink:type="resource" xlink:label="variable" 
     bindAsSequence="false"/>

    <!--  Scenario filter for the fact variable -->
    <ssf:scenario xlink:type="resource" xlink:label="filter" test="./eg:m"/>

    <!--  Labels for the formula, variable and filter -->
    <label:label xlink:type="resource" xlink:label="formula_label" 
      xlink:role="http://www.xbrl.org/2008/role/label" xml:lang="en">Returns the fact matched by the filter.</label:label>
    <label:label xlink:type="resource" xlink:label="variable_label" 
      xlink:role="http://www.xbrl.org/2008/role/label" xml:lang="en">Binds to the fact of which the context contains the scenario that has the child element "eg:m"</label:label>
    <label:label xlink:type="resource" xlink:label="filter_label" 
      xlink:role="http://www.xbrl.org/2008/role/label" xml:lang="en">Filters by checking whether the context contains the scenario that has the specified child element.</label:label>

    <!-- Arcs from labels to the formula, variable and filter being labelled -->
    <generic:arc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/element-label"
      xlink:from="formula" xlink:to="formula_label" order="1.0"/>    
    <generic:arc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/element-label"
      xlink:from="variable" xlink:to="variable_label" order="1.0"/>    
    <generic:arc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/element-label"
      xlink:from="filter" xlink:to="filter_label" order="1.0"/>    
 
    <!--  Arc from the formula to the variable it depends on -->
    <variable:variableArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-set"
      name="v:v" xlink:from="formula" xlink:to="variable" order="1.0"/>

    <!--  Arc from variable to the value filter -->
    <variable:variableFilterArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2008/variable-filter" 
      complement="false" cover="true"
      xlink:from="variable" xlink:to="filter" order="1.0"/>

  </generic:link>

</link:linkbase>
