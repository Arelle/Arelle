<testcase
xmlns="http://xbrl.org/2008/conformance"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
  http://xbrl.org/2008/conformance 
  ../../../infrastructure/test.xsd">
  <creator>
    <name>Herm Fischer</name>
    <email>fischer@markv.com</email>
  </creator>
  <number>51210</number>
  <name>Tuple Parent Filter Processing</name>
  <description>
    Test tuple parent filter processing.
  </description>
  <reference specification="TUPLE-FILTER" id="sec-parent-filter"/>

  <variation id="V-01">
    <name>Test of parent filter for root item</name>
    <description>
        Facts must not be contained by a tuple. Parent QName xbrli:xbrl.
    </description>
    <reference specification="TUPLE-FILTER" id="sec-parent-filter"/>
    <data>
      <schema>51210-tuple-test-concepts.xsd</schema>
      <linkbase>51210-parentAtRoot-formula.xml</linkbase>
      <instance readMeFirst="true">51210-parentAtRoot-instance.xml</instance>
    </data>
    <result>
      <instance>51210-parentAtRoot-result.xml</instance>
    </result>
  </variation>

  <variation id="V-02">
    <name>Test of parent filter for child of tuple</name>
    <description>
        Facts must be contained by a specified tuple.
    </description>
    <reference specification="TUPLE-FILTER" id="sec-parent-filter"/>
    <data>
      <schema>51210-tuple-test-concepts.xsd</schema>
      <linkbase>51210-parentInTuple-formula.xml</linkbase>
      <instance readMeFirst="true">51210-parentInTuple-instance.xml</instance>
    </data>
    <result>
      <instance>51210-parentInTuple-result.xml</instance>
    </result>
  </variation>

</testcase>

