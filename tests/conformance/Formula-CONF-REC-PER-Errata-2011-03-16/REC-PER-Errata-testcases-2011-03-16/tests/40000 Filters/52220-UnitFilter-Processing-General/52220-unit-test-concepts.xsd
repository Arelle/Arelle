<schema 
  xmlns="http://www.w3.org/2001/XMLSchema" 
  elementFormDefault="qualified" 
  targetNamespace="http://xbrl.org/formula/conformance/example"
  xmlns:eg="http://xbrl.org/formula/conformance/example"
  xmlns:xbrli="http://www.xbrl.org/2003/instance" 
  xmlns:xlink="http://www.w3.org/1999/xlink" 
  xmlns:xl="http://www.xbrl.org/2003/XLink"
  xmlns:link="http://www.xbrl.org/2003/linkbase" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  >

  <import 
    namespace="http://www.xbrl.org/2003/instance" 
    schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>

  <element 
    name="a" 
    id="a" 
    xbrli:periodType="instant"
    type="xbrli:decimalItemType" 
    substitutionGroup="xbrli:item" 
    abstract="false" 
    nillable="true" />

  <element 
    name="b" 
    id="b" 
    xbrli:periodType="instant"
    type="xbrli:stringItemType" 
    substitutionGroup="xbrli:item" 
    abstract="false" 
    nillable="true" />

  <!-- measures for units -->
  <element 
    name="squareMiles" 
    id="squareMiles" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="acres" 
    id="acres" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="hectares" 
    id="hectares" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="miles" 
    id="miles" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="feet" 
    id="feet" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="kilometers" 
    id="kilometers" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="meters" 
    id="meters" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="hours" 
    id="hours" 
    type="xs:string" 
    abstract="true"/>
  <element 
    name="seconds" 
    id="seconds" 
    type="xs:string" 
    abstract="true"/>

</schema>
