<testcase
xmlns="http://xbrl.org/2008/conformance"
xmlns:xbrlfe="http://www.xml.org/2008/formula/error" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="
  http://xbrl.org/2008/conformance 
  ../../../infrastructure/test.xsd">
  <creator>
    <name>Takahide Muramoto, Hitoshi Okumura, Shigehisa Miyano</name>
    <email>taka.muramoto@jp.fujitsu.com, okmr@jp.fujitsu.com, s-miyano@jp.fujitsu.com</email>
  </creator>
  <number>48230</number>
  <name>Period-End Filter Processing</name>
  <description>
    Test period-end filter processing
  </description>
  <reference specification="PERIOD-FILTER" id="sec-period-end-filter"/>

  <variation id="V-01">
    <name>Test filter by comparing the end period's date.</name>
    <description>The fact must be reported with a finite duration that ends at the end of the day on March 31, 2007.</description>
    <reference specification="PERIOD-FILTER" id="eg-period-end-filters"/>
    <data>
      <schema>48230-periodEndFilter-concepts.xsd</schema>
      <linkbase>48230-periodEndFilter-date-formula.xml</linkbase>
      <instance readMeFirst="true">48230-periodEndFilter-date-instance.xml</instance>
    </data>
    <result>
      <instance>48230-periodEndFilter-result.xml</instance>
    </result>
  </variation>

  <variation id="V-02">
    <name>Test filter by comparing both the end period's date and its time.</name>
    <description>The fact must be reported with a finite duration that ends at the start of the day on April 1, 2007.</description>
    <reference specification="PERIOD-FILTER" id="eg-period-end-filters"/>
    <data>
      <schema>48230-periodEndFilter-concepts.xsd</schema>
      <linkbase>48230-periodEndFilter-datetime-formula.xml</linkbase>
      <instance readMeFirst="true">48230-periodEndFilter-datetime-instance.xml</instance>
    </data>
    <result>
      <instance>48230-periodEndFilter-result.xml</instance>
    </result>
  </variation>

</testcase>

