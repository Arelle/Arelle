    </period>
    <scenario>
      <xbrldi:explicitMember dimension="eg:dim2">eg:dim2Mem1</xbrldi:explicitMember>
    </scenario>
  </context>
  
  <context id="c-2008-d2-m1">
    <entity>
      <identifier scheme="http://xbrl.org/entity/identification/scheme">01</identifier>
    </entity>
    <period>
      <instant>2008-12-31</instant>
    </period>
    <scenario>
      <xbrldi:explicitMember dimension="eg:dim1">eg:dim1Mem1</xbrldi:explicitMember>
      <xbrldi:explicitMember dimension="eg:dim2">eg:dim2Mem1</xbrldi:explicitMember>
