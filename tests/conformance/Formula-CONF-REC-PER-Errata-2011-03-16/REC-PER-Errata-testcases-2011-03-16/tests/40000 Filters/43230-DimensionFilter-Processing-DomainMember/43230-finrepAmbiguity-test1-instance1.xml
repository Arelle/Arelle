  <!-- these n1's are not intended to bind to v1, and represent
       the finrep situation -->
  <eg:n1 contextRef="c-2008-d2-m1" unitRef="unit" decimals="0">71113</eg:n1>
  <eg:n1 contextRef="c-2008-d2-m1" unitRef="unit" decimals="0">81113</eg:n1>
  <eg:n1 contextRef="c-2008-d2-m1" unitRef="unit" decimals="0">91113</eg:n1>

  <!-- n2 to n4 are pri items in 2dimRole ext link -->
  <eg:n2 contextRef="c-2008-d2-m1" unitRef="unit" decimals="0">0011</eg:n2>
  <eg:n3 contextRef="c-2008-d2-m2" unitRef="unit" decimals="0">0101</eg:n3>
  <eg:n4 contextRef="c-2008-d2-m3" unitRef="unit" decimals="0">1001</eg:n4>
    
