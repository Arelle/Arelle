  <creator>
    <name>Herm Fischer</name>
    <email>fischer@markv.com</email>
  </creator>
  <number>41220</number>
  <name>Boolean Or Filter Processing</name>
  <description>
    Test boolean or filter processing.
  </description>
  <reference specification="BOOLEAN-FILTER" id="sec-or-filter"/>

  <variation id="V-01">
    <name>Test or'ing of two concept name filters</name>
    <description>
      Facts matching the or-filter must be named &lt;eg:c&gt; or
      &lt;eg:d&gt;; this excludes fact named eg:e from participating.
      These or conditions must be covering (to prevent implicit filters
      from excluding any solution).
    </description>
    <reference specification="BOOLEAN-FILTER" id="sec-or-filter"/>
    <data>
      <schema>41220-orFilter-concepts.xsd</schema>
      <linkbase>41220-nameOrName-formula.xml</linkbase>
      <instance readMeFirst="true">41220-nameOrName-instance.xml</instance>
    </data>
    <result>
      <instance>41220-nameOrName-result.xml</instance>
    </result>
  </variation>

</testcase>

