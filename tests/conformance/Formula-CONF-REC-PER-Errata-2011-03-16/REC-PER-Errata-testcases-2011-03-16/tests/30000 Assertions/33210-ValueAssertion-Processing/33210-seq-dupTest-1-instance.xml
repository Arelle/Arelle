    </entity>
    <period>
      <instant>2007-12-31</instant>
    </period>
  </context>

  <unit id="u-moon-count">
    <measure>pure</measure>
  </unit>
  


    <!-- Earth moon fails precondition, not tested -->
    <concept:Moon contextRef="context-earth">Moon</concept:Moon>

    <!-- this count passes precondition and is duplicated, passes assertion -->
    <concept:MoonCount decimals="0" contextRef="context-earth" unitRef="u-moon-count">1</concept:MoonCount>
    <concept:MoonCount decimals="0" contextRef="context-earth-twin" unitRef="u-moon-count">1</concept:MoonCount>

    <!-- deimos moon is duplicated here, passes precond with Phobos and fail assertion 
         because not EVERY moon is duplicated -->
    <concept:Moon contextRef="context-mars">Phobos</concept:Moon>
    <concept:Moon contextRef="context-mars">Deimos</concept:Moon>
    <concept:Moon contextRef="context-mars-green-men">Deimos</concept:Moon>

    <!-- fails assertion for this concept & context -->
    <concept:MoonCount decimals="0" contextRef="context-mars" unitRef="u-moon-count">1</concept:MoonCount>

    <!-- passes precondition but not duplicated -->
    <concept:Moon contextRef="context-jupiter">Metis</concept:Moon>
