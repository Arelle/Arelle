  <variation id="V-01">
    <name>Consistency check a = b * c, strict, reported valid a, evaluated ok</name>
    <description>
      Consistency check valid a = b * c.  Evaluates consistently.
    </description>
    <reference specification="CONSISTENCY-ASSERTION" id="sec-syntax-ca-resources"/>         
    <data>
      <schema>31210-consistencyAssertion-concepts.xsd</schema>
      <linkbase>31210-strict-abc-formula.xml</linkbase>
      <instance readMeFirst="true">31210-strict-abc-evaluated-instance.xml</instance>
    </data>
    <result>
      <assertionTests 
         assertionID="assertion"
         countSatisfied="2"
         countNotSatisfied="0" />
      <instance>31210-a-bc-result.xml</instance>
    </result>
  </variation>

