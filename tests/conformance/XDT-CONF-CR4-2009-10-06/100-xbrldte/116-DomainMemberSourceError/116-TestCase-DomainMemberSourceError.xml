<?xml version="1.0" encoding="UTF-8"?><!-- Copyright 2005 XBRL International. All Rights Reserved. --><?xml-stylesheet type="text/xsl" href="../../testcase.xsl"?><testcase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xbrldte="http://xbrl.org/2005/xbrldt/errors" xmlns="http://xbrl.org/2005/conformance" name="116-TestCase-DomainMemberSourceError" description="2.1 to 2.10 - Dimensional relationships except 2.5" outpath="out" owner="ihr@xbrl.org" minimal="true" xsi:schemaLocation="http://xbrl.org/2005/conformance ../../lib/test.xsd">
        <!-- Dimensional taxonomy syntax rules pertaining to schemas and linkbases, and not covered by XML Schema and XBRL Validation. -->
        <!-- Note that XBRL validation requires the source and target of a definitionArc to be a concept. -->
        <!-- As of the 2005-10-26 internal working draft. (Herm) -->
        <!-- domainMember valid is same as dimensionDomainValid -->
        <!-- 2.5.3.1  1. A dimensional processor MUST raise an error [Dim Err, 19] xbrldte:DomainMemberSourceError if the source of a domain-member arc is not a primary item declaration -->
        <variation id="V-01" name="sourceDomainMemberIsDimensionInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.5.3.1.1">The source of a domain-member relationship is a dimension item declaration, not a primary item.</description>
                <data>
                        <xsd readMeFirst="true">sourceDomainMemberIsDimensionInvalid.xsd</xsd>
                        <linkbase readMeFirst="false">sourceDomainMemberIsDimensionInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">sourceDomainMemberIsDimensionInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:DomainMemberSourceError</error>
                </result>
        </variation>
        <variation id="V-02" name="sourceDomainMemberIsHypercubeInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.5.3.1.1">The source of a domain-member relationship is a hypercube item declaration, not a primary item.</description>
                <data>
                        <xsd readMeFirst="true">sourceDomainMemberIsHypercubeInvalid.xsd</xsd>
                        <linkbase readMeFirst="false">sourceDomainMemberIsHypercubeInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">sourceDomainMemberIsHypercubeInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:DomainMemberSourceError</error>
                </result>
        </variation>
</testcase>
