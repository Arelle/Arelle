                        <linkbase readMeFirst="false">sourceHasHypercubeIsDimensionSubInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">sourceHasHypercubeIsDimensionSubInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:HasHypercubeSourceError</error>
                </result>
        </variation>

