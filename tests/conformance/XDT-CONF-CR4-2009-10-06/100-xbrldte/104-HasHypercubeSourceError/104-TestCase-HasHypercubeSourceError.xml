                <data>
                        <xsd readMeFirst="true">hasHypercubeAllAbsPriItemValid.xsd</xsd>
                        <linkbase readMeFirst="false">hasHypercubeAllAbsPriItemValid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">hasHypercubeAllAbsPriItemValid-label.xml</linkbase>
                </data>
                <result/>
        </variation>
        <variation id="V-04" name="hasHypercubeAllTwoCubesValid">
                <description reference="XDT-IWD-2005-10-26.doc#2.2">Two primary items, parent with one hasHypercube child with another to different hypercube, both segment.</description>
                <data>
                        <xsd readMeFirst="true">hasHypercubeAllTwoCubesValid.xsd</xsd>
                        <linkbase readMeFirst="false">hasHypercubeAllTwoCubesValid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">hasHypercubeAllTwoCubesValid-label.xml</linkbase>
                </data>
                <result/>
        </variation>
        <variation id="V-05" name="hasHypercubeAllSubsValid">
                <description reference="XDT-IWD-2005-10-26.doc#2.2">A primary items substituting for an xbrli:item has an all hypercube arc to an element substituting for a xbrldt:hypercubeItem.</description>
                <data>
                        <xsd readMeFirst="true">hasHypercubeAllSubsValid.xsd</xsd>
                        <linkbase readMeFirst="false">hasHypercubeAllSubsValid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">hasHypercubeAllSubsValid-label.xml</linkbase>
                </data>
                <result/>
        </variation>
        <!-- 2.3.1.1 1. A dimensional processor MUST raise an error [Dim Err, 4] xbrldte:HasHypercubeSourceError if the source of an "all" or "notAll" relationship is not a primary item declaration -->
        <variation id="V-06" name="sourceHasHypercubeIsDimensionInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.3.1.1">The source of a hasHypercube relationship is not a primary item declaration, in this case it is a dimension item.</description>
                <data>
                        <xsd readMeFirst="true">sourceHasHypercubeIsDimensionInvalid.xsd</xsd>
                        <linkbase readMeFirst="false">sourceHasHypercubeIsDimensionInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">sourceHasHypercubeIsDimensionInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:HasHypercubeSourceError</error>
                </result>
        </variation>
        <variation id="V-07" name="sourceHasHypercubeIsDimensionSubInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.3.1.1">The source of a hasHypercube relationship is not a primary item declaration, in this case it substitutes for a dimension item.</description>
                <data>
