<?xml version="1.0" encoding="UTF-8"?><!-- Copyright 2005 XBRL International. All Rights Reserved. --><?xml-stylesheet type="text/xsl" href="../../testcase.xsl"?><testcase xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xbrldte="http://xbrl.org/2005/xbrldt/errors" xmlns="http://xbrl.org/2005/conformance" name="113-TestCase-DimensionDomainTargetError" description="2.1 to 2.10 - Dimensional relationships except 2.5" outpath="out" owner="ihr@xbrl.org" minimal="true" xsi:schemaLocation="http://xbrl.org/2005/conformance ../../lib/test.xsd">
        <!-- Dimensional taxonomy syntax rules pertaining to schemas and linkbases, and not covered by XML Schema and XBRL Validation. -->
        <!-- Note that XBRL validation requires the source and target of a definitionArc to be a concept. -->
        <!-- As of the 2005-10-26 internal working draft. (Herm) -->
        <!-- 2.5.3.1.1 2. A dimensional processor MUST raise an error [Dim Err, 17] xbrldte:DimensionDomainTargetError if the target of the relationship is not a domain member declaration [Def, 6] -->
        <variation id="V-01" name="targetDimensionDomainIsDimensionInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.5.3.1.1">The target of a dimension-domain relationship is a dimension item declaration, not a domain member.</description>
                <data>
                        <xsd readMeFirst="true">targetDimensionDomainIsDimensionInvalid.xsd</xsd>
                        <linkbase readMeFirst="false">targetDimensionDomainIsDimensionInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">targetDimensionDomainIsDimensionInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:DimensionDomainTargetError</error>
                </result>
        </variation>
        <variation id="V-02" name="targetDimensionDomainIsHypercubeInvalid">
                <description reference="XDT-IWD-2005-10-26.doc#2.5.3.1.1">The target of a dimension-domain relationship is a hypercube item declaration, not a domain member item.</description>
                <data>
                        <xsd readMeFirst="true">targetDimensionDomainIsHypercubeInvalid.xsd</xsd>
                        <linkbase readMeFirst="false">targetDimensionDomainIsHypercubeInvalid-definition.xml</linkbase>
                        <linkbase readMeFirst="false">targetDimensionDomainIsHypercubeInvalid-label.xml</linkbase>
                </data>
                <result >
                        <error>xbrldte:DimensionDomainTargetError</error>
                </result>
        </variation>
</testcase>
